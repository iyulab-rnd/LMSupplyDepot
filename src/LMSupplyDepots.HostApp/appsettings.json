{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "LMSupplyDepots": "Information"
        }
    },
    "AllowedHosts": "*",
    "LMSupplyDepots": {
        "ModelsDirectory": "",
        "MaxConcurrentDownloads": 2,
        "VerifyChecksums": true,
        "MinimumFreeDiskSpace": 10737418240,
        "HuggingFaceApiToken": "",
        "DefaultTimeoutMs": 60000,
        "MaxConcurrentOperations": 4,
        "EnableMetrics": true,
        "EnableModelCaching": true,
        "MaxCachedModels": 2,
        "TempDirectory": "",
        "ForceCpuOnly": false,
        "LLamaOptions": {
            "Threads": null,
            "GpuLayers": 0,
            "ContextSize": 4096,
            "BatchSize": 512,
            "AntiPrompt": [ "User:", "Assistant:", "\n\n" ],
            "UseMemoryMapping": true,
            "Seed": null
        }
    }
}