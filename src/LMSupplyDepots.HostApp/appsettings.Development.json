{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Information",
            "LMSupplyDepots": "Debug"
        }
    },
    "LMSupplyDepots": {
        "ModelsDirectory": "",
        "MaxConcurrentDownloads": 2,
        "VerifyChecksums": true,
        "MaxConcurrentOperations": 4,
        "EnableMetrics": true,
        "ForceCpuOnly": true,
        "LLamaOptions": {
            "Threads": 4,
            "GpuLayers": 0,
            "ContextSize": 4096
        }
    }
}